
@model AdminPanel.Models.Dashboard

@{
    ViewBag.Title = "Dashboard";
}

<head>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Dashboard.css" rel="stylesheet" type="text/css" />
</head>

<div class="page-title">
    <h2>Dashboard</h2>
    <h3>Welcome, @HttpContext.Current.Session["username"]</h3>
</div>

<div class="server-stats-parent">
    <div class="total-money-parent">
        <h2>@Model.totalMoney</h2>
        <h3>Total money</h3>
    </div>

    <div class="total-players-parent">
        <h2>@Model.totalPlayers</h2>
        <h3>Total players</h3>
    </div>

    <div class="total-vehicles-parent">
        <h2>@Model.totalVehicles</h2>
        <h3>Total vehicles</h3>
    </div>
</div>